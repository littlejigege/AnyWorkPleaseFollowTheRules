<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:epv="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.qgstudio.anywork.widget.ExamPagerView xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/epv"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        epv:sLeftBottomBackgroundColor="@color/sample_blue"
        epv:sLeftBottomTextColor="@color/white"
        epv:sLeftBottomTextString="保存"
        epv:sRightBottomBackgroundColor="#23BE78"
        epv:sRightBottomTextColor="@color/white"
        epv:sRightBottomTextString="提交"
        epv:sTitleBackgroundColor="@color/sample_blue"
        epv:sTitleCenterTextColor="@color/sample_blue"
        epv:sTitleCenterTextString=""
        epv:sTitleLeftIconResId="@drawable/icon_register_back"
        epv:sTitleRightIconResId="@drawable/icon_answer_answercard"
        epv:sTitleRightIconTittle="答题卡" />

</FrameLayout>
